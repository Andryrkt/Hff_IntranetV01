{% extends "baseTemplate.html.twig" %}

{% block title %}
	Nouvelle agence
{% endblock %}

{% block breadCrumb %}{% endblock %}

{% block content %}
	<div class="container my-4">
		<div class="card">
			<div class="card-body">
				<div class="container">
					<h3 class="perso-titre">
						Formulaire Cr√©ation Agence
					</h3>
					<a href="{{path("agence_index")}}" class="btn btn-sm  text-white my-3 fw-bold" style="background-color: #FBBB01;">Liste des agences</a>
					{{ form_start(form) }}
					{{ form_errors(form)}}
					<div class="row">
						<div class="col-12 col-md-6">
							{{ form_row(form.codeAgence) }}
						</div>
						<div class="col-12 col-md-6">
							{{ form_row(form.libelleAgence) }}
						</div>
						{{ form_row(form.services) }}
					</div>
					<button type="submit" class="btn btn-primary">
						<i class="fas fa-save"></i>
						Enregistrer
					</button>
					{{ form_end(form) }}
				</div>
			</div>
		</div>
	</div>
{% endblock %}

{% block javascript %}
	<script src="{{ App.base_path }}/Views/js/admin/agence/agence.js?v=2025.10.22.08.00"></script>
{% endblock %}
