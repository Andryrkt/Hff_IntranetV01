{% extends "baseTemplate.html.twig"%}

{% block breadCrumb %}{% endblock %}

{% block content %}
	<div class="container my-4">
		<h2 class="text-center fs-1 fw-bold bg-black py-2" style="color: #FBBB01">
			Modification Agence Service Autoriser
		</h2>

		<a href="{{path("autoriser_index")}}" class="btn btn-sm  text-white my-3 fw-bold" style="background-color: #FBBB01;">Liste</a>

		{{ form_start(form) }}
		{{ form_errors(form)}}

		<div class="row">
			<div class="col-12 col-md-6">

				{{ form_row(form.Session_Utilisateur) }}

			</div>
			<div class="col-12 col-md-6">

				{{ form_row(form.Code_AgenceService_IRIUM) }}

			</div>
		</div>


		<button type="submit" class="btn btn-primary">
			<i class="fas fa-save"></i>
			Enregistrer
		</button>
		{{ form_end(form) }}
	</div>
{% endblock %}
