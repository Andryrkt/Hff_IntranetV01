<div class="email-header">
	{{ numDa }}
	- 
	{% if statut in ['validationDa', 'validationAteDa'] %}
		PROPOSITION(S) 
		<span class="validationDa">
			VALIDÉE(S) PAR L’
			{% if tab.service is same as 'atelier' %}
				ATELIER
			{% elseif tab.service is same as 'appro' %}
				APPRO
			{% endif %}
		</span>
	{% else %}
	DEMANDE D'APPROVISIONNEMENT
	<span class="{{ statut }}">
		{% if statut == "newDa" %}
			CRÉÉ
		{% elseif statut == 'bonAchatDa' %}
			BON D'ACHAT
		{% elseif statut == 'propositionDa' %}
			PROPOSITION
		{% elseif statut == 'modificationDa' %}
			MODIFICATION
		{% elseif statut == 'demandeDeverouillage' %}
			DEMANDE DE DÉVERROUILLAGE
		{% elseif statut == 'confirmationDeverrouillage' %}
			DEMANDE DÉVERROUILLÉE
		{% endif %}
	</span>
	{% endif %}
</div>
