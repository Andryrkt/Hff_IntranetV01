<div class="email-header">
	{{ numDa }}
	- DEMANDE D'APPROVISIONNEMENT
	<span class="{{ statut }}">
		{% if statut == "validationDa" %}
			VALIDÉE
		{% elseif statut == "newDa" %}
			CRÉÉ
		{% elseif statut == 'bonAchatDa' %}
			BON D'ACHAT
		{% elseif statut == 'propositionDa' %}
			PROPOSITION
		{% elseif statut == 'modificationDa' %}
			MODIFICATION
		{% elseif statut == 'validationAteDa' %}
			VALIDÉE PAR L'ATE
		{% elseif statut == 'demandeDeverouillage' %}
			DEMANDE DE DÉVERROUILLAGE
		{% elseif statut == 'confirmationDeverrouillage' %}
			DEMANDE DÉVERROUILLÉE
		{% endif %}
	</span>
</div>
