<div class="accordion" id="accordionExample">
	<div class="accordion-item">
		<h2 class="accordion-header">
			<button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
				Formulaire de recherche
			</button>
		</h2>
		<div id="collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
			<div class="accordion-body">
				{{ form_start(form)}}
				<div class="row">

					<div class="col-12 col-md-2">
						{{form_row(form.agence)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.agenceDebite)}}
						{{form_row(form.serviceDebite)}}

					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.interneExterne)}}
						{{form_row(form.typeligne)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.facture)}}
						{{form_row(form.numOr)}}
						{{form_row(form.numSerie)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.plan)}}
						{{form_row(form.idMat)}}
						{{form_row(form.numParc)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.annee)}}
						{{form_row(form.dateDebut)}}
						{{form_row(form.dateFin)}}
					</div>
				</div>
			</class>
			<div class="d-flex justify-content-end mb-2">
				<button class="btn btn-warning fw-bold" type="submit">
					<i class="fab fa-searchengin"></i>
					Recherche</button>
			</div>
			{{ form_end(form)}}
		</div>
	</div>
</div></div>
