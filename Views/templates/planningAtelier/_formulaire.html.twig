<div class="accordion" id="formAccordion">
	<div class="accordion-item">
		<h2 class="accordion-header">
			<button class="accordion-button enteteAccordion" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
				Formulaire de recherche
			</button>
		</h2>
		<div id="collapseOne" class="accordion-collapse collapse" data-bs-parent="#formAccordion">
			<div class="accordion-body">
				{{ form_start(form, { 'attr': { 'id': 'planning_atelier_search_form' } }) }}

				<div class="row">
					<div class="col-12 col-md-2">
						{{form_row(form.agenceEm)}}
						{{form_row(form.agenceDebite)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.numeroSemaine)}}
						{{form_row(form.serviceDebite)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.dateDebut)}}
						{{form_row(form.dateFin)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.numOr)}}
						{{form_row(form.resource)}}
					</div>
					<div class="col-12 col-md-2">
						{{form_row(form.section)}}
					</div>

				</div>
			</div>
			<div class="d-flex justify-content-end mb-2">
				<button class="btn btn-warning ajout-overlay fw-bold" id="btn_search" type="submit">
					<i class="fab fa-searchengin"></i>
					Recherche</button>
			</div>
			{{ form_end(form)}}

		</div>


	</div>
</div>
